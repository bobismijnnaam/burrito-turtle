	a[0] = 'H';
	a[1] = 'e';
	a[2] = 'l';
	a[3] = 'l';
	a[4] = 'o';
	a[5] = ' ';
	a[6] = 'w';
	a[7] = 'o';
	a[8] = 'r';
	a[9] = 'l';
	a[10] = 'd';
	
	int i = 0; i < 11 @
		a[i]|;
		i+;
	.
	
	|\\;
	
	<- 0;
.