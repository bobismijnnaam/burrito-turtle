int showLlama() 
	char[2] line1;
	line1[0] = '~';
	line1[1] = ')';
	
	char[6] line2;
	line2[0] = '(';
	line2[1] = '_';
	line2[2] = '-';
	line2[3] = '-';
	line2[4] = '-';
	line2[5] = ';';
    
    char[6] line3;
    line3[0] = ' ';
    line3[1] = '/';
    line3[2] = '|';
    line3[3] = '~';
    line3[4] = '|';
    line3[5] = '\';
    
    char[6] line4;
    line4[0] = '/';
    line4[1] = ' ';
    line4[2] = '/';
    line4[3] = ' ';
    line4[4] = '/';
    line4[5] = '|';

	
	int i = 0; i < 2 @
		line1[i]|;
		i+;
	.
	
	|\;
	
	i = 0; i < 6 @
		line2[i]|;
		i+;
	.
	
	|\;
	
	i = 0; i < 6 @
		line3[i]|;
		i+;
	.
	
	|\;
	
	i = 0; i < 6 @
		line4[i]|;
		i+;
	.

	<- 0;
.

int helloWorld() 
