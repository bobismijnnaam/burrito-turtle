int program()
	-- Multi Dimensional Array test!
	int[2][2][2][2] x;
	int a = 0;
	int b = 0;
	int c = 0;
	int d = 0;

	a < 2 @ 
		b = 0;
		b < 2 @
			c = 0;
			c < 2 @
				d = 0;
				d < 2 @
					x[a][b][c][d] = a + b + c + d;
					x[a][b][c][d]|;
					d = d + 1;
				.
				c = c + 1;
			.
			b = b + 1;
		.
		a = a + 1;
	.
	
	<- 0;
.